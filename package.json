    "check:ci": "biome ci src/",
    "pre-commit": "biome check --write src/ && vue-tsc --noEmit",
    "prepare": "husky",
    "commit": "git-cz",
    "commitlint": "commitlint --from=HEAD~1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
