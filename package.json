  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "tauri": "tauri",
    "format": "biome format --write src/",
    "lint": "biome lint src/",
    "lint:fix": "biome lint --write src/",
    "check": "biome check src/",
